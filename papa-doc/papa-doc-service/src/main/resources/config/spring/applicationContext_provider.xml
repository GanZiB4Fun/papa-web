<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <!-- dubbo config -->
    <dubbo:protocol name="dubbo" port="30980" dispatcher="all"
                    threadpool="fixed" threads="500"/>
    <!--  <dubbo:protocol name="hessian" port="20981"/>-->
    <!-- <dubbo:protocol name="dubbo" port="20880" server="netty" client="netty" codec="dubbo" serialization="hessian2"
       charset="UTF-8" threadpool="fixed" threads="100" queues="0" iothreads="9" buffer="8192" accepts="1000" payload="8388608"/> -->
    <dubbo:application name="logistics-order-platform2" logger="log4j"/>

    <dubbo:registry protocol="zookeeper" address="${zookeeper.addr}"/>

    <dubbo:service protocol="dubbo" retries="0" interface="com.ganzib.papa.doc.service.IAppDocumentService"
                   ref="appUserService" timeout="60000"/>
</beans>
